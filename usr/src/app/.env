# Current Environment (development | test | production)
NEXT_PUBLIC_NODE_ENV=development

# IMPORTANT: You must replace these placeholder values with your actual Firebase project secrets.
# You can find these in your Firebase project settings:
# Project Settings > General > Your apps > Firebase SDK snippet > Config

# Firebase Client SDK Configuration
NEXT_PUBLIC_FIREBASE_API_KEY="__YOUR_API_KEY__"
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN="__YOUR_AUTH_DOMAIN__"
NEXT_PUBLIC_FIREBASE_PROJECT_ID="__YOUR_PROJECT_ID__"
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET="__YOUR_STORAGE_BUCKET__"
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID="__YOUR_MESSAGING_SENDER_ID__"
NEXT_PUBLIC_FIREBASE_APP_ID="__YOUR_APP_ID__"

# Firebase Admin SDK Configuration
# IMPORTANT: This is a JSON object and must be enclosed in single quotes if it contains special characters.
# Go to Project Settings > Service accounts > Generate new private key to get this JSON file.
FIREBASE_SERVICE_ACCOUNT_KEY='{"type": "service_account", "project_id": "__YOUR_PROJECT_ID__", "private_key_id": "__YOUR_PRIVATE_KEY_ID__", "private_key": "-----BEGIN PRIVATE KEY-----\\n...\\n-----END PRIVATE KEY-----\\n", "client_email": "__YOUR_CLIENT_EMAIL__", "client_id": "__YOUR_CLIENT_ID__", "auth_uri": "https://accounts.google.com/o/oauth2/auth", "token_uri": "https://oauth2.googleapis.com/token", "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs", "client_x509_cert_url": "__YOUR_CLIENT_CERT_URL__"}'

# Whinself Interceptor URL (if you are using one)
WHINSELF_API_URL="https://your-ngrok-or-interceptor-url.com"
